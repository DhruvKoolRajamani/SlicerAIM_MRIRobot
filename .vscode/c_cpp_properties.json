{
    "env": {
        "MESHLAB_BIN_DIR": "/usr/bin/"
    },
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/build/**",
                "${workspaceFolder}/build/WorkspaceGeneration/**",
                "${env:Slicer_BUILD_DIR}/Slicer-build/**",
                "${env:Slicer_BUILD_DIR}/**"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c11",
            "intelliSenseMode": "${default}",
            "configurationProvider": "ms-vscode.cmake-tools",
            "cppStandard": "c++11",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "browse": {
                "path": [
                    "${workspaceFolder}/",
                    "${workspaceFolder}/build/"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}